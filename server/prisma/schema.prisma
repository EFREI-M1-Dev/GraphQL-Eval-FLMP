generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        Int     @default(autoincrement()) @id
  username  String  @unique
  password  String
}

model Article {
  id      Int       @default(autoincrement()) @id
  title   String
  content String
}
